<script language="JavaScript" type="text/javascript">	
	function replace(str,searchFor,replaceWith){
		x = str.indexOf(searchFor);
		while(x!=-1){
	    	j = x+searchFor.length;
	    	str = str.substring(0,x)+replaceWith+str.substring(j,str.length);
	    	x = str.indexOf(searchFor,x+replaceWith.length);
		}
		return str;
	}
	//TESTING for www4 issue
	function translateEscaper(varName){
		varName = replace(varName,"&quot\;","\"");
		varName = replace(varName,"&gt\;",">");
		varName = replace(varName,"&lt\;","<");
		varName = replace(varName,"&#39;","'");
		varName = replace(varName,"&amp;","&");
		varName = replace(varName,"&~!","+");
	
		return varName;
	}
	
	function doPrompt(url) {
		var professor = "Berenbrink"+" "+"Petra";
		professor = translateEscaper(professor);		
		var msgText='Copy the URL below into your RSS feed to see ratings for - "' + professor + '"';
	    prompt(msgText, url)
	}	

	function RatingWarning(selfLogin) {
		if(selfLogin) {
			alert('Sorry, but as a professor, you are not allowed to rate yourself.');
		} else {
			alert('Sorry, but as a professor, you are not allowed to rate other professors.');
		}
	}
	
</script>

