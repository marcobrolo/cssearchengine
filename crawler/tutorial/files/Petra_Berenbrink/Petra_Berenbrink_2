	<script language="JavaScript1.2" type="text/javascript">
	var IE = document.all?true:false;
	if (!IE) document.captureEvents(Event.MOUSEMOVE)	
	document.onmousemove = getMouseXY;
	var tempX=0,tempY=0;
	function getMouseXY(e) {
		

		if (IE) { // grab the x-y pos.s if browser is IE
			tempX = event.clientX + document.documentElement.scrollLeft;
			tempY = event.clientY + document.documentElement.scrollTop;	
		}
		else {  // grab the x-y pos.s if browser is NS
			tempX = e.pageX;
			tempY = e.pageY;
		}  

		if (tempX < 0){
			tempX = 0;
		}
		if (tempY < 0){
			tempY = 0;
		}  
		return true;
	}
	function hideShow()
	{
		var elem = document.getElementById('rmp_reb_notice');
		if(elem.style.display=="none"){			
			tempX = tempX - 600;
			tempY = tempY - 300;
			elem.style.top=tempY+'px';
			elem.style.left=tempX+'px';	
			elem.style.display="block";						
		} else {
			elem.style.display="none";
		}		
	}
	function showRebuttals() {
		mywindow=window.open('/ShowProfessorRebuttal.jsp?tid=36606','mywindow',
		'width=440,height=225,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,copyhistory=no,resizable=no');		
		var screenY = screen.height;
		var screenX = screen.width;		
		var leftvar = (screenX/2) - 220
		var rightvar = (screenY/2) - 112
		mywindow.moveTo(leftvar,rightvar);
		mywindow.focus();
	}
</script>
