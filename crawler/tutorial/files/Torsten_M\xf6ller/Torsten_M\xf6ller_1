function dupeRatingCheck(){
var y_n = true;
	if (xmlHttp.readyState==4){ 

		if(xmlHttp.responseText==0){
			y_n = false;

			document.body.style.cursor = 'default';
			document.RatingInputForm.submit = false;
		}
	}
	return y_n;
}
		
function trim(inputString){
		while(inputString.charAt(0)==" "){
			inputString=inputString.replace(inputString.charAt(0),"");
		}
		while(inputString.charAt((inputString.length -1))==" "){
			inputString=inputString.substring(0,inputString.length-1);
		}
		return inputString;
	}
	 
  function GetXmlHttpObject()
{
var xmlHttp=null;
try
  {
  // Firefox, Opera 8.0+, Safari
  xmlHttp=new XMLHttpRequest();
  }
catch (e)
  {
  // Internet Explorer
  try
    {
    xmlHttp=new ActiveXObject("Msxml2.XMLHTTP");
    }
  catch (e)
    {
    xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");
    }
  }
return xmlHttp;
}


function validString(inputString) {

	for (i=0 ; i < inputString.length ; i++) {
		if (onlyCharacters(inputString.charAt(i)) == false) {
			return false;
		}
	}
	return true;
}

function validNumber(inputString) {

	for (i=0 ; i < inputString.length ; i++) {
		if (onlyNumbers(inputString.charAt(i)) == false) {
			return false;
		}
	}
	return true;
}

function validAlphaNumeric(inputString) {

	var isValidAlphaNumeric=true;
	//changes made for RMPM-642
	//if ( ! (inputString.match(/^[A-Za-z0-9]+(\.[A-Za-z0-9]{1,2})?$/) ) ) {
	//	isValidAlphaNumeric=false;
	//}
	if ( ! (inputString.match(/^[A-Za-z0-9 .-]*$/) ) ) {
		isValidAlphaNumeric=false;
	}
	
	return isValidAlphaNumeric;
}

function validISBN(inputString) {

	var isValidISBN=true;
	
	for (i=0; i < inputString.length; i++) {
		if ( !( (inputString.charAt(i) >= '0' && inputString.charAt(i) <= '9') ||
			   (inputString.charAt(i) == 'x' || inputString.charAt(i) == 'X') ) ) {
			isValidISBN=false;
		}
	}
	return isValidISBN;
}

function onlyCharacters(entry)
{
	if ( (entry >= 'a' && entry <= 'z') || ( entry >='A' && entry <='Z') )
		return true;
	else
		return false;
}

function onlyNumbers(entry)
{
	if ( (entry >= '0' && entry <= '9') )
		return true;
	else
		return false;
} 

//-->
