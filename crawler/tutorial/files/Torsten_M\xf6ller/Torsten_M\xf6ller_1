var limit = 350;
var limitMessage='You cannot have more than 350 characters for a comment.'
function validate_() {
    var f = document.RatingInputForm;
    var checked = false;
    for (var i = 0; i < f.interestrb.length; i++) {
	    if (f.interestrb[i].checked) {
	    	checked = true;
	    }
    }
    if (!checked) {
	    alert("Please select an 'Interest' rating");
	    return false;
    }
    checked = false;
    for (var i = 0; i < f.easyrb.length; i++) {
	    if (f.easyrb[i].checked) {
	    	checked = true;
	    }
    }
    if (!checked) {
	    alert("Please select an 'Easiness' rating");
	    return false;
    }
    checked = false;
    for (var i = 0; i < f.helpfulrb.length; i++) {
	    if (f.helpfulrb[i].checked) {
	    	checked = true;
	    }
    }
    if (!checked) {
	    alert("Please select a 'Helpfulness' rating");
	    return false;
    }
    checked = false;
    for (var i = 0; i < f.clarityrb.length; i++) {
	    if (f.clarityrb[i].checked) {
	    	checked = true;
	    }
    }
    if (!checked) {
	    alert("Please select a 'Clarity' rating");
	    return false;
    }

    checked = false;
    for (var i = 0; i < f.textbookuserb.length; i++) {
	    if (f.textbookuserb[i].checked) {
	    	checked = true;
	    }
    }
    if (!checked) {
	    alert("Please select a 'Textbook Use' rating");
	    return false;
    }	

	/*if (f.ccodept1.value == 'CLASS'){
