		alert( "Please enter a 'Class Code #'." );
		f.ccodept2.focus();
		return false ;
	}
	if (f.ccodept2.value == 'SECT'){
		alert( "Please enter a 'Class Code #'." );
		f.ccodept2.focus();
		return false ;
	}*/

    if ($.trim(f.ccodept1.value).length == 0) {
    	alert("Please enter 'Course & Section Code'.");
	    return false;
    }

    /*if (f.ccodept2.value.length == 0) {
    	alert("Please enter a 'Class Code #'.");
	    return false;
    }*/

    if ( !validAlphaNumeric(f.ccodept1.value) ) {
    	alert("Please enter 'Course & Section code' with only letters [AaBbCc...] ,numbers [012...], dot [.], space[ ] and hypehn [-].");
	    return false;
    }

	/*if ( !validAlphaNumeric(f.ccodept2.value) ) {
    	alert("Please enter a 'Class Code #' with only letters [AaBbCc...] and numbers [012...] with no period at the start and end");
	    return false;
    }*/
	
    if ( f.textbookisbn.value.length > 0 ) {   
	    if(!(f.textbookisbn.value.length==10 || f.textbookisbn.value.length==13)){
	        alert("ISBN# must be 10 or 13 digits long.");
	        return false;
	    }
    	 if (!validISBN(f.textbookisbn.value) ) {
	    	alert("Please enter a valid 'Textbook ISBN#' with only numbers [012...], letter [Xx] and it must be 10 or 13 digits long.");
		    return false; 
	    }
	    return true;
    }
    
	
    /* if (f.ccode.value.length == 0) {
    	alert("Please enter a 'Course'");
	    return false;
    } */

    if (trim(f.comments.value).length == 0) { // RMPM-886 related changes
    	/* alert("Please enter a comment about this teacher's teaching ability (required for profs with 3 or more ratings)."); */
    	alert("Please enter a comment about this Professor.");
		document.getElementById("comments").select();
	return false;
    }
    /* Included to character limit RMPM-436*/
    if (f.comments.value.length > limit) {
    	/* alert("Please enter a comment about this teacher's teaching ability (required for profs with 3 or more ratings)."); */
    	alert(limitMessage);
		return false;
   }
    /*included to filter bad words, RMPM-439*/
    var badword = true;
    badword = badwordsCheck('Comments',f.comments.value);
	if(!badword){
		return false;
	}
	
	if (document.getElementById("code").value == '') {
		alert('Please enter the characters from the displayed image');
		document.getElementById("code").value = '';
		document.getElementById("code").focus();
		return false;
	}
		checkcode(document.getElementById("code").value);
		return false;
	

}

 function updateCount() {
   var old = document.RatingInputForm.counter.value;
   document.RatingInputForm.counter.value=document.RatingInputForm.comments.value.length;
   if (document.RatingInputForm.counter.value > limit && old <= limit) {
     alert(limitMessage);
     if (document.styleSheets) {
       document.RatingInputForm.counter.style.fontWeight = 'bold';
       document.RatingInputForm.counter.style.color = '#ff0000'; 
       document.RatingInputForm.comments.style.color = '#ff0000'; 
     } 
   } else if (document.RatingInputForm.counter.value <= limit && old > limit
	       && document.styleSheets ) {
       document.RatingInputForm.counter.style.fontWeight = 'normal';
       document.RatingInputForm.counter.style.color = '#000000'; 
       document.RatingInputForm.comments.style.color = '#000000'; 
   } 
 }
 function disallowCR(evt) {
    var keyCode = document.layers ? evt.which : evt.keyCode;
    if (keyCode == 13) {
        return false;
    }
    return true;
 }
