<script type="text/javascript">
var pageValue = location.toString();
var nameExt = ".jsp";

if(pageValue.indexOf("bbpress") >-1 )
{
if(pageValue.charAt(pageValue.lastIndexOf("/")+1) == "")
{
var pn = location.toString().substring(location.toString().lastIndexOf("//")+2, location.toString().length)+"home"+nameExt;
var h2 = "forum/home";
}
else
{
var pn = location.toString().substring(location.toString().lastIndexOf("//")+2, location.toString().length)+nameExt;
var h2 = pageValue.substring(pageValue.indexOf(".com")+5, pageValue.length);
}
var channel= "forum";
}
else if(pageValue.indexOf("homepage") >-1 )
{
if(pageValue.charAt(pageValue.lastIndexOf("/")+1) == "")
{
var pn = location.toString().substring(location.toString().lastIndexOf("//")+2, location.toString().length)+"home"+nameExt;
var h2 = "home/home";
}
else
{
var pn = location.toString().substring(location.toString().lastIndexOf("//")+2, location.toString().length)+nameExt;
var h2 = pageValue.substring(pageValue.indexOf(".com")+5, pageValue.length);
}
var channel= "home";
}

btg.config.Demdex.enabled = false;
btg.config.DoubleClick.enabled = true;
btg.config.DoubleClick.dartSite = 'rmp.mtvi';
btg.config.DoubleClick.positionThreshold=2;

btg.config.Omniture["trackExternalLinks"]= true;
btg.config.Omniture["pageName"]= pn;
btg.config.Omniture["channel"]= channel;
btg.config.Omniture["hier2"]= h2;
btg.config.Omniture["prop2"]= "USA";
btg.config.Omniture["prop8"]= "visitor";

//Initialize Controller

//Send a page call
btg.Controller.sendPageCall();
</script>
