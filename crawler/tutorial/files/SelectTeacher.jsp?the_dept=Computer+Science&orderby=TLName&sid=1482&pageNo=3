<script type="text/javascript">
var contentStr = "<div id='tipdiv'><p style='font-weight:bold;font-size:15px;'>Tips:<a style=\"float:right;background:no-repeat;background-image:url(/images/site/xmark.png);padding:20px 20px 0 0\" onclick=\"javascript:$('#qtip-0').hide(); return false;\" href=\"#\"></a><span></p>";
/* contentStr += "<ul style=\"vertical-align:middle;float:left\">";
contentStr += "<li style=\"display:inline\"><img src=\"/images/site/tip-green.png\" style=\"vertical-align:middle\" /></li><li style=\"display:inline\">To <b>search</b> for a professor at this school, enter his or her last name and click \"Search.\"</li>";*/
contentStr +="<img src=\"/images/site/tip-green.png\" style=\"vertical-align:middle;float:left;padding-right:5px;padding-top:5px;\">To <b>search</b> for a professor at this school, enter his or her last name and click \"Search.\" ";
/*contentStr += "<li style='list-style-image:url(\"/images/site/tip-green.png\")'>To <b>browse by letter,</b> simply enter your desired letter and click \"Search.\"</li>"; */
// contentStr +="</ul></div>";
contentStr +="</div>";

function showTip(showReady){
	$('.nameSearch').qtip({
		content: contentStr,
		style: {
			classes: 'qtip-shadow qtip-bootstrap',
			tip: {
				border: 6,
				height: 10
			}
		},
		position: {
			target: $('.nameSearch')
		},
		show : {
			event: 'load',
			ready: showReady
		},
		hide : {
			event: 'onclick'
		}	
	});
}
/*
 * Creates Cookie for specified number of days.
 *
 * name - Cookie Name
 * value - Cookie Value
 * days - Number of days after which Cookie will expire.
 * createCookie("COOKIE_NAME",COOKIE_VALUE,365);
 */
function createCookie(name, value, days) {
  var expires = "";
  if(days) {
   var date = new Date();
    var timeToexpire = days * 24 * 60 * 60 * 1000;
    date.setTime(date.getTime() + timeToexpire);
    expires = "; expires=" + date.toGMTString();
  }
  document.cookie = name + "=" + value + expires + "; path=/";
}
/*
 *  Reads Cookie based on Cookie name.
 *  name - Cookie name
 *  readCookie("COOKIE_NAME");
 */
function readCookie(name) {
  var nameEQ = name + "=";
  var ca = document.cookie.split(';');
  for(var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while(c.charAt(0) == ' ')
    c = c.substring(1, c.length);
    if(c.indexOf(nameEQ) == 0)
      return c.substring(nameEQ.length, c.length);
  }
  return null;
}

	var loadCookieVal = readCookie("FIRST_TIME_LOAD");
	if(loadCookieVal == null){
		// No cookie value
		showTip(true);
		createCookie("FIRST_TIME_LOAD","YES",365);
	} else {
		// cookie value exists
		showTip(false);		
	}
</script>
