<script>
	//alert(document.getElementById("upperAdvertisement").innerHTML.length);
	//alert(document.getElementById("lowerAdvertisement").innerHTML.length);
	//alert(document.getElementById("rightAdvertisement").innerHTML.length);
	if (document.getElementById("rightAdvertisement") != null
			&& document.getElementById("rightAdvertisement") != undefined
			&& document.getElementById("rightAdvertisement").innerHTML.length < 2000) {
		document.getElementById("rightAdvertisement").style.display = "none";
		document.getElementById("rightAdvertisementImg").style.display = "none";
	}
	if (document.getElementById("upperAdvertisement") != null
			&& document.getElementById("upperAdvertisement") != undefined
			&& document.getElementById("upperAdvertisement").innerHTML.length < 2000) {
		document.getElementById("upperAdvertisement").style.display = "none";
		document.getElementById("upperAdvertisementImg").style.display = "none";
	}
	if (document.getElementById("lowerAdvertisement") != null
			&& document.getElementById("lowerAdvertisement") != undefined
			&& document.getElementById("lowerAdvertisement").innerHTML.length < 2000) {
		document.getElementById("lowerAdvertisement").style.display = "none";
		document.getElementById("lowerAdvertisementImg").style.display = "none";
	};
</script>
