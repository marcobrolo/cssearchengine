<script type="text/javascript" charset="utf-8">


        function getSitename() {
          return "rmp";
        }

        function getTerm() {
               if (document.getElementById("query") && document.getElementById("query").value){
                  return document.getElementById("query").value;
               }else if (document.getElementById("query1") && document.getElementById("query1").value){
                  return document.getElementById("query1").value;
               }else if (document.getElementById("query2") && document.getElementById("query2").value){
                  return document.getElementById("query2").value;
               }else{
                  return "*:*";
               }
        }

        function getValue(max, type) {
               var columns = max.split("~~");
                  var x = 0;
                  var output = "";
                  for (x=0; x<columns.length; x++) {
                      if (columns[x].indexOf(type) != -1) {
                           output = "";
                           output = columns[x].substring(columns[x].indexOf(":") + 1, columns[x].length);
                           return output;
                          }
                  }
                  return max;
        }

      function getSelectedValue(max, type) {
             var columns = max.split("~~");
                  var x = 0;
                  for (x=0; x<columns.length; x++) {
                      if (columns[x].indexOf(type) != -1) {
                                  if(columns[columns.length-1].indexOf(type)!=-1){
                                     return columns[x].substring(columns[x].indexOf(":") + 1, columns[x].length);
                                  }else{
                                     return columns[x].substring(columns[x].indexOf(":") + 1, columns[x].length) + columns[columns.length-1];
                                  }
                          }
                  }
                  return max;
        }

        function getHelp(){
           var $helpDialog = $('#helpDialog').dialog({
                                height: 440,
                                width: 450,
                                modal: true,
                                position: 'center',
                                autoOpen: false,
                                title: 'Search Help',
                                closeText:'xx'
           });
           $helpDialog.show();
           $helpDialog.dialog("open");

        }
 
        function getParameter( name ){
          name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
          var regexS = "[\\?&]"+name+"=([^&#]*)";
          var regex = new RegExp( regexS );
          var results = regex.exec( window.location.href );
          if( results == null )    return "";
          else    return (results[1]).replace(/\+/g,' ');
        }


        function beforeSearch(formname,sinput,soption){
          // TODO: Store value of the form that was submitted
          var sterm = $('#'+sinput).val() || '*:*';
          var stype = $('#'+soption).val() || 'TEACHER';
          reportSearch(sterm,stype);
          //reportPageCall(sterm,stype); // it is being reported via omnitureExtension.jsp
        }

        function reportSearch(sterm,stype){
           com.mtvi.reporting.Controller.sendLinkEvent({
                   linkName:"SEARCH",
                   linkType:"o",
                   eVar2:sterm,
                   eVar3:stype
           });
        }

        function reportFacetClick(sterm,stype){
           com.mtvi.reporting.Controller.sendLinkEvent({
                   linkName:"FACET",
                   linkType:"o",
                   eVar2:sterm,
                   eVar3:stype
           });
        }

        function reportFailedSearch(sterm,stype){
           com.mtvi.reporting.Controller.sendLinkEvent({
                   linkName:"SEARCH",
                   linkType:"o",
                   eVar3:stype,
                   eVar4:sterm
           });
        }

        function reportPageCall(sterm,stype){
                btg.Controller.sendPageCall({
                   pageName : location.pathname,
                   channel : "search",
                   hier2 : "search/"+stype,
                   eVar2 : sterm,
                   eVar3 : stype,
                   prop2 : "",
                   prop4 : "",
                   prop5 : "",
                   prop6 : "",
                   prop7 : "",
                   prop8 : ""
                });

        }

        function hideSolrElements(){
           $("#docs").hide();
           $("#navigation").hide();
           $(".facetDiv").hide();
        }

        function showSolrElements(){
           $("#docs").show();
           $("#navigation").show();
           $(".facetDiv").show();
        }

$(document).ready( function() {
/*
        $('select[name^=queryoption]').each(function(){
            $(this).change(function(){
                 val = $('option:selected',this).text();
                 $(this).next().text(val);
            });
        });
*/
 });

</script>
