var status=0;
if (status < 0){document.write('<li id="hot" class="status1 tTip" title="Is your professor hot?  Hot professors get a red chili pepper"><strong>0</strong><span>Hotness</span></li>');}
else if (status == 0){document.write('<li id="hot" class="status1 tTip" title="Is your professor hot?  Hot professors get a red chili pepper"><strong>0</strong><span>Hotness</span></li>');}
else if (status < 10){document.write('<li id="hot" class="status2 tTip" title="Is your professor hot?  Hot professors get a red chili pepper"><strong>0</strong><span>Hotness</span></li>');}
else if (status < 20){document.write('<li id="hot" class="status3 tTip" title="Is your professor hot?  Hot professors get a red chili pepper"><strong>0</strong><span>Hotness</span></li>');}
else if (status >= 20){document.write('<li id="hot" class="status4 tTip" title="Is your professor hot?  Hot professors get a red chili pepper"><strong>0</strong><span>Hotness</span></li>');}
else{}
