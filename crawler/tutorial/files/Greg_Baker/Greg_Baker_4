  $(document).ready(function() {
        $("#query1").bind("webkitspeechchange", function (e) {
            //Send a link event call
            btg.Controller.sendLinkEvent(
               {
                  linkName:'speechsearch',
                  linkType:'o'
               }
            );
        });
